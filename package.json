{
  "name": "axios-logger",
  "version": "3.0.0",
  "description": "Beautify Axios Logging Messages",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "type": "module",
  "exports": {
    "./lib": {
      "types": "./types/index.d.ts" ,
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    }
  },
  "keywords": [
    "javascript",
    "nodejs",
    "network",
    "axios",
    "http",
    "log",
    "logger",
    "beautify",
    "library",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hg-pyun/axios-logger.git"
  },
  "author": "Haegul Pyun",
  "email": "phg2491@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hg-pyun/axios-logger/issues"
  },
  "homepage": "https://github.com/hg-pyun/axios-logger#readme",
  "devDependencies": {
    "@types/dateformat": "^5.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "dateformat": "^5.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:clean && npm run build:ts",
    "build:clean": "rm -rf ./lib",
    "build:ts": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "test": "jest --silent",
    "test:watch": "jest --watchAll",
    "test:server": "node ./test/server.js",
    "deploy": "npm test && npm publish"
  }
}
