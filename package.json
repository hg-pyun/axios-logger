{
  "name": "axios-logger",
  "version": "2.6.2",
  "description": "Beautify Axios Logging Messages",
  "main": "lib/index.js",
  "keywords": [
    "javascript",
    "nodejs",
    "network",
    "axios",
    "http",
    "log",
    "logger",
    "beautify",
    "library",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hg-pyun/axios-logger.git"
  },
  "author": "Haegul Pyun",
  "email": "phg2491@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hg-pyun/axios-logger/issues"
  },
  "homepage": "https://github.com/hg-pyun/axios-logger#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/dateformat": "^5.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "dateformat": "^5.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "rm -rf ./lib && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "cross-env BABEL_ENV=production babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "test": "jest --silent",
    "test:watch": "jest --watchAll",
    "test:server": "node ./test/server.js",
    "deploy": "npm test && npm publish"
  }
}
